        private method fileExists, boolean
            required in path, string
            endparams
        proc
            try
            begin
                data ch, int, 0
                open(ch,i,path)
                close ch
                mreturn true
            end
            catch (e)
            begin
                mreturn false
            end
            endtry
        endmethod

