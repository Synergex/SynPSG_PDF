.noproto
.ifdef OS_VMS
; Move the address of the descriptor for the second argument
; into the first argument.
subroutine set_dscr
a_arg,		D_ADDR
^ref(a_dsc),	[*]i4
proc
	a_arg = ^addr(a_dsc)
	xreturn
endsubroutine
.else
.ifndef DBLNET
function libhpdf_dllname, A
static record
	ostyp,	i4, 0
proc
	if (.not. ostyp)
	    xcall envrn(ostyp)
	if (ostyp .eq. 6) then
	    freturn("LIBHPDF")
	else if (ostyp .eq. 8) then
.if D_ADDRSIZE .eq. 8
	    freturn("libhpdf64.so")
.else
	    freturn("libhpdf.so")
.endc
	else
.if D_ADDRSIZE .eq. 8
	    freturn("libhpdf64.dll")
.else
	    freturn("libhpdf.dll")
.endc
endfunction
.endc
.endc
